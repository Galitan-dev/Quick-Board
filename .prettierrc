{
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 100,
    "plugins": ["prettier-plugin-svelte"],
    "pluginSearchDirs": ["."],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
    "arrowParens": "always",
    "bracketSpacing": true,
    "svelteIndentScriptAndStyle": true,
    "tabWidth": 4
}
